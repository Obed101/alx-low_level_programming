#include "holberton.h"
/**
 * *_strncat - concatenates at least 'n' bytes of two strings
 * @dest: destination
 * @src:  source pointer
 * @n: bytes number
 * Return: nothing
*/

char *_strncat(char *dest, char *src, int n)
{

int d_len, i;
for (d_len = 0; dest[d_len] != '\0'; d_len++)
;

for (i = 0; i < n && src[i] != '\0'; i++)
	dest[dest_len + i] = src[i];

/*should end with a end of string char*/
dest[dest_len + i] = '\0';

return (dest);
}
