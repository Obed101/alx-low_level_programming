#include "holberton.h"

/**
 *
 *
 *
 *
 */
char *string_nconcat(char *s1, char *s2, unsigned int n)
{
	s1 = malloc (sizeof(s1));
	s2 = realloc (s1, n);

	if (s1 == NULL || s2 == NULL)
	{
		return(NULL);
	}
	string_nconcat(*s1, *s2, n);

	if (n >= strlen(s2))
	{
		return (s2);
	}
	if (s1 == NULL || s2 == NULL)
	{
		s1 = {}
}
